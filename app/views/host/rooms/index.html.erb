<p id="notice"><%= notice %></p>

<h1>Host Rooms</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @host_rooms.each do |host_room| %>
      <tr>
        <td><%= link_to 'Show', host_room %></td>
        <td><%= link_to 'Edit', edit_host_room_path(host_room) %></td>
        <td><%= link_to 'Destroy', host_room, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Host Room', new_host_room_path %>
